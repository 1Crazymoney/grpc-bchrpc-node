{
  "name": "grpc-bchrpc-node",
  "version": "0.2.0",
  "description": "Bitcoin Cash gRPC interface for node.js clients.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "mkdir -p 'pb' && grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./pb --grpc_out=./pb --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ./ ./*.proto && protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./pb -I ./ ./*.proto"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcramer/grpc-bchrpc-node.git"
  },
  "keywords": [
    "bch"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcramer/grpc-bchrpc-node/issues"
  },
  "homepage": "https://github.com/jcramer/grpc-bchrpc-node#readme",
  "devDependencies": {
    "@types/google-protobuf": "^3.7.1",
    "@types/node": "^12.0.10",
    "grpc_tools_node_protoc_ts": "^2.5.4",
    "mkdirp": "^0.5.1"
  },
  "dependencies": {
    "grpc": "^1.21.1",
    "protobufjs": "^6.8.8"
  }
}
