{
  "name": "grpc-bchrpc-node",
  "version": "0.10.0",
  "description": "BCHD gRPC interface for node.js clients.",
  "main": "index.js",
  "files": [
    "*.d.ts",
    "src/*.js",
    "pb/"
  ],
  "scripts": {
    "lint": "tslint --project tsconfig.json --format stylish",
    "test": "tsc && nyc mocha",
    "build": "./node_modules/.bin/grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./ --grpc_out=./ --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ./ ./pb/*.proto && ./node_modules/grpc-tools/bin/protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./ -I ./ ./pb/*.proto && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simpleledgerinc/grpc-bchrpc-node.git"
  },
  "keywords": [
    "bch",
    "BCHD",
    "rpc",
    "grpc",
    "bitcoin",
    "cash"
  ],
  "author": "James Cramer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcramer/grpc-bchrpc-node/issues"
  },
  "homepage": "https://github.com/jcramer/grpc-bchrpc-node#readme",
  "devDependencies": {
    "@types/mocha": "^5.1.1",
    "@types/node": "^12.7.2",
    "grpc-tools": "^1.8.0",
    "grpc_tools_node_protoc_ts": "^2.5.4",
    "mocha": "^5.1.1",
    "mocha-steps": "^1.3.0",
    "typescript": "^3.8.3",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.7.0",
    "nyc": "^14.1.1",
    "@istanbuljs/nyc-config-typescript": "^0.1.3"
  },
  "dependencies": {
    "@types/google-protobuf": "^3.7.1",
    "google-protobuf": "^3.9.1",
    "grpc": "^1.24.2",
    "protobufjs": "^6.8.8"
  }
}
