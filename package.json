{
  "name": "grpc-bchrpc-node",
  "version": "0.5.5",
  "description": "BCHD gRPC interface for node.js clients.",
  "main": "index.js",
  "files": [
    "index.ts",
    "src/",
    "pb/"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./ --grpc_out=./ --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ./ ./pb/*.proto && protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./ -I ./ ./pb/*.proto && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcramer/grpc-bchrpc-node.git"
  },
  "keywords": [
    "bch",
    "BCHD",
    "rpc",
    "bitcoin"
  ],
  "author": "James Cramer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcramer/grpc-bchrpc-node/issues"
  },
  "homepage": "https://github.com/jcramer/grpc-bchrpc-node#readme",
  "devDependencies": {
    "grpc_tools_node_protoc_ts": "^2.5.4"
  },
  "dependencies": {
    "google-protobuf": "^3.2.0",
    "grpc": "^1.21.1",
    "protobufjs": "^6.8.8",
    "@types/google-protobuf": "^3.7.1",
    "@types/node": "^12.0.10"
  }
}
